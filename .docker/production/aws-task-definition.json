{
  "family": "fezproduction",
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "uqlibrary/docker-nginx-fez:4",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8007,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "links": [
        "fpm:fpm"
      ],
      "volumesFrom": [
        {
          "sourceContainer": "fpm"
        }
      ],
      "essential": true
    },
    {
      "name": "fpm",
      "image": "uql-registry.library.uq.edu.au/production/fez:<COMMIT_HASH>",
      "cpu": 256,
      "memory": 1024,
      "environment": [
        {
          "name": "APP_ENVIRONMENT",
          "value": "production"
        },
        {
          "name": "COMMIT_HASH",
          "value": "<COMMIT_HASH>"
        },
        {
          "name": "JAVA_HOME",
          "value": "/usr/lib/jvm/jre"
        },
        {
          "name": "JHOVE_HOME",
          "value": "/usr/local/jhove"
        },
        {
          "name": "NEWRELIC_LICENSE",
          "value": "<NEWRELIC_LICENSE>"
        },
        {
          "name": "WEBCRON_TOKEN",
          "value": "<WEBCRON_TOKEN>"
        }
      ],
      "essential": true
    }
  ],
  "volumes": []
}